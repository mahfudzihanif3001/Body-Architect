# .env.example
# Copy this file to .env and fill real secret values.
# DO NOT commit your real .env to source control.

# Server
PORT=3000
NODE_ENV=development

# Database (Postgres URI)
DATABASE_URL=postgres://<DB_USER>:<DB_PASSWORD>@<DB_HOST>:5432/<DB_NAME>

# JWT secret used to sign tokens
JWT_SECRET=your_jwt_secret_here

# Google OAuth (for /google-login)
GOOGLE_CLIENT_ID=your_google_client_id_here

# Generative AI / Gemini API key (used by generate-plan)
GEMINI_API_KEY=your_generative_ai_key_here

# Spoonacular (nutrition lookup) - optional but recommended
SPOONACULAR_API_KEY=your_spoonacular_api_key_here

# RapidAPI key (if used by third-party lookups)
RAPIDAPI_KEY=your_rapidapi_key_here

# Optional: override DB options or other feature flags
# FEATURE_X_ENABLED=false
